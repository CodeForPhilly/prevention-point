{
  "name": "prevpoint-uds-frontend",
  "version": "1.0.0",
  "description": "Prevention Point URS frontend app",
  "main": "src/index.js",
  "author": "Code for Philly",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.15.2"
  },
  "scripts": {
    "dev": "node devServer.js",
    "build": "parcel build public/index.html",
    "test": "jest",
    "lint": "eslint src && prettier --check \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "stylelint",
      "git add"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.1.1",
    "apisauce": "^1.0.2",
    "axios-auth-refresh": "^1.0.6",
    "babel-polyfill": "^6.26.0",
    "clsx": "^1.0.4",
    "material-table": "^1.39.2",
    "mobx": "^5.9.4",
    "mobx-react-lite": "2.0.7",
    "moment": "^2.24.0",
    "query-string": "^6.9.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^9.1.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "mobx-react-devtools": "^6.1.1",
    "parcel-bundler": "^1.12.3",
    "parcel-proxy-server": "^1.1.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.6",
    "sass": "^1.18.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.4"
  }
}
